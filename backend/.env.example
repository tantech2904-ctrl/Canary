# Backend environment variables
APP_NAME=RegimeShift Sentinel
ENV=development
SECRET_KEY=supersecretkey
ACCESS_TOKEN_EXPIRE_MINUTES=480
CORS_ALLOW_ORIGINS=http://localhost:5173,http://localhost:3000
# For Docker Compose, the hostname is "postgres"; for local dev use 127.0.0.1
POSTGRES_DSN=postgresql+asyncpg://rs_user:rs_pass@postgres:5432/rs_db
# For Docker Compose, the hostname is "redis"; for local dev use 127.0.0.1
REDIS_URL=redis://redis:6379/0
RATE_LIMIT_PER_MINUTE=120
